{
  "version": 2,
  "builds": [
    {
      "src": "parking-front/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/parking-front/$1"
    }
  ],
  "buildCommand": "cd parking-front && npm run build",
  "outputDirectory": "parking-front/build",
  "installCommand": "cd parking-front && npm install"
}
